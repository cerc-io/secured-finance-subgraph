{
  "name": "secured-finance-subgraph",
  "version": "0.2.0-beta.34",
  "private": "true",
  "scripts": {
    "generate": "ts-node ./scripts/generate.ts",
    "codegen": "graph codegen ./subgraph.yaml",
    "precodegen": "ts-node ./scripts/copy-abis.ts",
    "build": "graph build ./subgraph.yaml",
    "prebuild": "npm run codegen",
    "test": "graph test",
    "coverage": "graph test -- -c",
    "auth": "graph auth --studio",
    "deploy:development": "graph deploy sf-protocol-dev-sepolia ./subgraph.development.yaml --studio --output-dir ./build/development --version-label",
    "deploy:development-arb": "graph deploy sf-protocol-dev-arb-sepolia ./subgraph.development-arb.yaml --studio --output-dir ./build/development-arb --version-label",
    "deploy:development-ava": "graph deploy sf-protocol-dev-ava-fuji ./subgraph.development-ava.yaml --studio --output-dir ./build/development-ava --version-label",
    "deploy:staging": "graph deploy sf-protocol-stg-sepolia ./subgraph.staging.yaml --studio --output-dir ./build/staging --version-label",
    "deploy:staging-arb": "graph deploy sf-protocol-stg-arb-sepolia ./subgraph.staging-arb.yaml --studio --output-dir ./build/staging-arb --version-label",
    "deploy:staging-ava": "graph deploy sf-protocol-stg-ava-fuji ./subgraph.staging-ava.yaml --studio --output-dir ./build/staging-ava --version-label",
    "deploy:sepolia": "graph deploy sf-prd-sepolia ./subgraph.sepolia.yaml --studio --output-dir ./build/sepolia --version-label",
    "deploy:mainnet": "graph deploy sf-prd-mainnet ./subgraph.mainnet.yaml --studio --output-dir ./build/mainnet --version-label",
    "deploy:arbitrum-sepolia": "graph deploy sf-prd-arbitrum-sepolia ./subgraph.arbitrum-sepolia.yaml --studio --output-dir ./build/arbitrum-sepolia --version-label",
    "deploy:arbitrum-one": "graph deploy sf-prd-arbitrum-one ./subgraph.arbitrum-one.yaml --studio --output-dir ./build/arbitrum-one --version-label",
    "deploy:avalanche-mainnet": "graph deploy sf-prd-avalanche-mainnet ./subgraph.avalanche-mainnet.yaml --studio --output-dir ./build/avalanche-mainnet --version-label",
    "deploy:polygon-zkevm-mainnet": "graph deploy sf-prd-polygon-zkevm-mainnet ./subgraph.polygon-zkevm-mainnet.yaml --studio --output-dir ./build/polygon-zkevm-mainnet --version-label",
    "prepublish": "npm run codegen",
    "update-subgraphrc": "ts-node ./scripts/update-subgraphrc.ts",
    "format": "prettier --config ./.prettierrc.json --check '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}'",
    "format:fix": "prettier --config ./.prettierrc.json --write '**/*.{js,jsx,ts,tsx,json}' '*.{js,jsx,ts,tsx,json}'"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.68.0",
    "@graphprotocol/graph-ts": "0.31.0",
    "@secured-finance/contracts": "1.1.1-beta.3",
    "@types/js-yaml": "4.0.5",
    "@types/node": "16.18.67",
    "js-yaml": "4.1.0",
    "matchstick-as": "0.6.0",
    "prettier": "2.8.3",
    "ts-node": "10.9.1"
  }
}
